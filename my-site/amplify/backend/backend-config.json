{
	"storage": {
		"dataBaselolman": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		}
	},
	"function": {
		"mySiteData": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "dataBaselolman",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		},
		"backTest": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "dataBaselolman",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	},
	"api": {
		"siteAPI": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "mySiteData",
					"attributes": [
						"Name",
						"Arn"
					]
				},
				{
					"category": "function",
					"resourceName": "backTest",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	}
}